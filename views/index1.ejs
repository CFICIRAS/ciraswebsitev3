<script>
	// Vérifier si l'élément est visible dans la vue du navigateur
	$.fn.visible = function (partial) {
		var $t = $(this),
			$w = $(window),
			viewTop = $w.scrollTop(),
			viewBottom = viewTop + $w.height(),
			_top = $t.offset().top,
			_bottom = _top + $t.height(),
			compareTop = partial === true ? _bottom : _top,
			compareBottom = partial === true ? _top : _bottom;
		return compareBottom <= viewBottom && compareTop >= viewTop;
	};

	// Fonction pour ajouter la classe 'animate__fadeInUp' aux éléments animés lorsqu'ils apparaissent dans la vue
	function addAnimationClass() {
		$(".animated").each(function (index) {
			const $this = $(this);
			if ($this.visible(true)) {
				$this.addClass("animate__fadeInUp");
			}
		});
	}

	// Appel initial de la fonction pour les éléments déjà visibles lors du chargement de la page
	addAnimationClass();

	// Appeler la fonction lorsque l'utilisateur fait défiler la page
	$(window).scroll(function () {
		addAnimationClass();
	});
</script>
